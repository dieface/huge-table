{
  "name": "@importio/huge-table",
  "version": "4.0.0",
  "description": "Table component to handle huge sets of data, based on Facebook's FixedDataTable",
  "main": "lib/index.js",
  "jsnext:main": "lib/index.js"
  "scripts": {
    "lint": "eslint .",
    "test": "mocha",
    "examples-watch": "webpack --config ./examples/webpack.config.js --progress --colors --watch",
    "examples-build": "webpack --config ./examples/webpack.config.js --progress --colors",
    "examples-serve": "serve -p 8080 examples/",
    "babel": "rm -rf dist/ && mkdir dist/ && babel src --out-dir dist",
    "less": "lessc src/HugeTable.less dist/HugeTable.css",
    "release": "npm run babel && npm run less",
    "test-circle": "MOCHA_FILE=$CIRCLE_TEST_REPORTS/file.xml mocha test --reporter mocha-junit-reporter",
    "npmrc": "printf \"//registry.npmjs.org/:_authToken=$NPM_AUTH_TOKEN\" > .npmrc"
  },
  "author": "Yacine Rezgui (@yrezgui)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/import-io/frontend-packages"
  },
  "dependencies": {
    "fixed-data-table": "^0.2.0",
    "keymirror": "^0.1.1",
    "react": "^0.13.3",
    "react-bootstrap": "^0.24.2",
    "react-portal": "^0.7.0"
  },
  "devDependencies": {
    "babel": "^5.8.21",
    "babel-core": "^5.8.21",
    "babel-eslint": "^4.0.5",
    "babel-loader": "^5.3.2",
    "bootstrap": "^3.3.5",
    "chai": "^3.0.0",
    "clean-css": "^3.4.4",
    "css-loader": "^0.14.4",
    "eslint": "^1.1.0",
    "eslint-plugin-babel": "^2.0.0",
    "eslint-plugin-react": "^3.2.1",
    "extract-text-webpack-plugin": "^0.8.1",
    "file-loader": "^0.8.4",
    "jsdom": "^5.4.3",
    "less": "^2.5.1",
    "less-loader": "^2.2.0",
    "mocha": "^2.2.5",
    "mocha-junit-reporter": "^1.6.1",
    "node-libs-browser": "^0.5.2",
    "serve": "^1.4.0",
    "sinon": "^1.15.3",
    "sinon-chai": "^2.8.0",
    "skin-deep": "^0.6.0",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.9.10"
  }
}
